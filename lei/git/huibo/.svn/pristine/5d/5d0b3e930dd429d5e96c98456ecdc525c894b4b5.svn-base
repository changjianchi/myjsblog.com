@charset "utf-8";
* {
    box-sizing: border-box;
}
html,
body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
code,
form,
fieldset,
legend,
input,
textarea,
p,
blockquote,
th,
td,
em,
button {
    margin: 0;
    padding: 0;
    list-style: none;
}
@font-face {
    font-family: 'TT0140M';
    src: url('../font/TT0140M.eot'); 
    src: url('../font/TT0140M.eot') format('embedded-opentype'), 
             url('../font/TT0140M.woff') format('woff'), 
             url('../font/TT0140M.ttf')  format('truetype'), 
             url('../font/TT0140M.svg') format('svg'); 
}
@font-face {
    font-family: 'Song';
    src: url('../font/Song.eot'); 
    src: url('../font/Song.ttf')  format('truetype'); 
}

body {
    font-size: 14px;
    color: #000;
	font-family: "TT0140M";
	/*font-family: "TT0140M","SimSun","STSong","宋体";*/
}
a{
    text-decoration: none;
    color: inherit;
}
.clear {
    clear:both;
}
em,i{
    font-style: normal;
}
.none{
    display: none;
}
a img{
    border:none;
}
html,body{
    width: 100%;
    height: 100%;
    overflow: hidden;
}
/*------------------------------------------------------------------*/

.ani-tr0{
    -webkit-transform:translate3d(0,50px,0);
    transform:translate3d(0,50px,0);
    opacity: 0;
    -webkit-transition:all 1s ease;
    transition:all 1s ease;
}
.ani-tr0.ani-tr{
    -webkit-transform:translate3d(0,0,0);
    transform:translate3d(0,0,0);
    opacity: 1;
}

.ani-mate{
    -webkit-transform:translate3d(0,30px,0);
    transform:translate3d(0,30px,0);
    opacity: 0;
    -webkit-transition:all .8s ease-out;
    transition:all .8s ease-out;
}
.ani-mate.active{
    -webkit-transform:translate3d(0,0,0);
    transform:translate3d(0,0,0);
    opacity: 1;
}

/*------------------------------------------------------------------*/

#wrapper {
    position: absolute;
    z-index: 1;
    top: 0;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #ccc;
    overflow: hidden;
}

#scroller {
    position: absolute;
    z-index: 1;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    width: 100%;
    -webkit-transform: translateZ(0);
    -moz-transform: translateZ(0);
    -ms-transform: translateZ(0);
    -o-transform: translateZ(0);
    transform: translateZ(0);
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-text-size-adjust: none;
    -moz-text-size-adjust: none;
    -ms-text-size-adjust: none;
    -o-text-size-adjust: none;
    text-size-adjust: none;
    background: #fff;
}

/*------------------------------------------------------------------*/
.table-a{
    display: table;
}
.table-cell{
    display: table-cell;
    text-align: center;
    vertical-align: middle;
    width: 100%;
    height: 100%;
}

.swiper-slide{
    overflow: hidden;
}
.swiper-3{
