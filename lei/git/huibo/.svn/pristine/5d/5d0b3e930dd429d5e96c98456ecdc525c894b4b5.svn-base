    margin:.25rem auto 0;
}

.huibo-menu{
    position: fixed;
    z-index: 11;
    top:0;
    left: 0;
    width: 100%;
    height: 0;
    background: #000;
    overflow: hidden;
    -webkit-transition:height 1s ease;
    transition:height 1s ease;
}
.huibo-menu-col{
    width: 100%;
    height: 1.04rem;
    overflow: hidden;
    padding:.3rem 0 0 .41rem;
}
.huibo-menu-col em{
    display: inline-block;
    width: auto;
    height: .38rem;
    line-height: .38rem;
    font-size: .38rem;
    /*font-family: "微软雅黑";*/
