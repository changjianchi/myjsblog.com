.huibo-6k .swiper-slide-active a span:after,
.huibo-6k .swiper-slide-prev a span:after,
.huibo-6k .swiper-slide-next a span:after{
    content: "";
    width: 100%;
    height: 100%;
    position: absolute;
    top:0px;
    left: 0;
    display: block;
    background: rgb(184,222,105);
    opacity: 0.95;
    -webkit-transition:all 0.5s ease;
    transition:all 0.5s ease;
}
.huibo-6k .swiper-slide-active a span:after{
