    /*-webkit-animation: huiboTopBj 5s ease-out both; 
    animation: huiboTopBj 5s ease-out both;*/ 
}
@-webkit-keyframes huiboTopBj{
    0%{transform:scale(1.5,1.5);}
    100%{transform:scale(1,1);}
}
@keyframes huiboTopBj{
    0%{transform:scale(1.5,1.5);}
    100%{transform:scale(1,1);}
}

