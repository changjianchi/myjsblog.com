    font-size: .46rem;
    margin-right: .08rem;
}
.menu-logo{
    width: 100%;
    height: 1.38rem;
    overflow: hidden;
}
.menu-logo img{
    width: auto;
    height: 100%;
    display: block;
    margin: 0 auto;
}
.huibo-menu-nav,.a-menu-last{
    width: 100%;
    height: auto;
    padding:0 1.93rem;
    margin-top: .94rem;
}
.huibo-menu-nav a,.a-menu-last a{
    display: block;
    width: 100%;
    height: .9rem;
    line-height: .9rem;
