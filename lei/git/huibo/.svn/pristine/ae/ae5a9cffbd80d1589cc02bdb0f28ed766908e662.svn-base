    <script>
        var cases = {};
        var swiper1 = new Swiper('.huibo-kv', {
            pagination: '.swiper-pagination',
            nextButton: '.swiper-button-next',
            paginationClickable: true,
            spaceBetween: 0,
            autoplay: false,
            speed: 600,
            loop : true,
            onSlideChangeStart: function(swiper){ 
                clearInterval(cases.spImg);
                cases.casesBottomI = $('.swiper-slide-active').find('.cases-bottom').find('i');
                cases.casesBottomI.removeClass('act');
                cases.casesBottomILen = cases.casesBottomI.length;
                cases.qH = 0,cases.spImg = setInterval(function(){
                    cases.casesBottomI.eq(cases.qH).addClass('act');
                    cases.qH++;
                    if(cases.qH>=cases.casesBottomILen){
                        clearInterval(cases.spImg);
                        $('.swiper-button-next').trigger('click');
                    }
                }, 3000);
            },
        });

        cases.casesBottom2 = $('.cases-bottom');
        cases.casesBottom2.each(function(){
            var thLen = $(this).length;
            $(this).find('i').each(function(i){
                $(this).css({
                    'z-index':thLen-i
                });
            });
        });

        cases.nav = $('.cases-nav').find('em');
        cases.swiper_pagination_bullet = $('.cases .swiper-pagination-bullet');
        cases.nav.on('click',function(){
            if($(this).hasClass('cases-nav-k')){
                if($(this).hasClass('active')){
                    $(this).removeClass('active');
                    $(this).parent().find('span').removeClass('act');
                }else{ 
                    qingchu();
                    $(this).addClass('active');
                    $(this).parent().find('span').addClass('act');
                }
            }else{
                qingchu();
            }
            cases.navData_in = Number($(this).attr('data-in'));
            cases.swiper_pagination_bullet.eq(cases.navData_in).trigger('click');
        });

        function qingchu(){
            cases.nav.removeClass('active');
            cases.nav.parent().find('span').removeClass('act');
        }

        var url_scor = Number(GetQueryString('scor'));
        if(url_scor != null){
            cases.swiper_pagination_bullet.eq(url_scor-1).trigger('click');
            console.log(url_scor)
        }

    </script>

