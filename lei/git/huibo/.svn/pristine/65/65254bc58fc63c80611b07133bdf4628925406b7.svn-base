    <script>
    $(function(){
        var aboutK4I = $('.about-k4-i');
        var swiperPaginationBullet = $('.swiper-cobe .swiper-pagination-bullet');
        aboutK4I.each(function(i){
            // var Down_i = aboutK4A.eq(i).offset().top;
            $(this).on('click',function(){
                $('.swiper-cobe .swiper-pagination-bullet').eq(i+1).trigger('click');
                console.log(i+1)
            });
        });
    });
    </script>

    <!-- Swiper JS -->
    <script src="js/swiper.min.js"></script> 
    <script src="js/swiper.animate1.0.2.min.js"></script>
    <script>
        var swiperf = new Swiper('.swiper-3', {
            pagination: '.swiper-cobe',
            direction: 'vertical',
            slidesPerView: 'auto',
            slidesPerView: 1,
            paginationClickable: true,
            noSwiping : true,
            noSwipingClass : 'stop-swiping',
        });

        var swiper2 = new Swiper('.huibo-6k', {
            pagination: '.swiper-pagination1',
            slidesPerView: 'auto',
            centeredSlides: true,
            paginationClickable: true,
            spaceBetween: 20,
            loop: true,
            autoplay: 6000,
        });

        var swiper1 = new Swiper('.huibo-kv', {
            pagination: '.swiper-pagination',
            paginationClickable: true,
            // nextButton: '.swiper-button-next',
            // prevButton: '.swiper-button-prev',
            spaceBetween: 0,
            autoplay: 6000,
            speed: 600,
            loop : true,
            onSlideChangeStart: function(swiper){ 
                var cd = swiper.activeIndex;
                $('.huibo-kv-text').removeClass('ani-b');
                $('.huibo-kv-text').eq(cd).addClass('ani-b');
            },
        });

    </script>

    <!-- iscorll -->
    <script src="js/iscroll.js"></script>
    <script type="text/javascript">
        var myScroll;
        function loaded () {
            myScroll = new IScroll('#wrapper', { 
                scrollX: true, 
                freeScroll: true ,
                preventDefaultException: { tagName: /^(INPUT|TEXTAREA|BUTTON|SELECT|A)$/ },
                click: true,
            });

            var scroller = $('#scroller');
            var swiperPaginationBullet = $('.swiper-cobe .swiper-pagination-bullet')
            scroller.on('touchend mouseup',function(){
                var thTop = $(this).offset().top;
                console.log(thTop)
                if(thTop>0){
                    swiperPaginationBullet.eq(1).trigger('click');
                }
                
            });

            var url_scor = Number(GetQueryString('scor'));
            if(url_scor != null){
                $('.swiper-cobe .swiper-pagination-bullet').eq(url_scor-1).trigger('click');
            }
        }
        document.addEventListener('touchmove', function (e) { e.preventDefault(); }, false);
    </script>

